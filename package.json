{
  "name": "@madebythread/thread-kit",
  "version": "1.3.1",
  "description": "Toolkit for sanity studio",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "license": "MIT",
  "author": "Kegan Sovay <kegan@madebythread.com>",
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "plugin-kit verify-package --silent && pkg-utils build --strict --check --clean",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "watch": "pkg-utils watch --strict"
  },
  "dependencies": {
    "@sanity/icons": "^3.4.0",
    "@sanity/incompatible-plugin": "^1.0.4",
    "@sanity/ui": "^2.8.6",
    "lucide-react": "^0.411.0",
    "react-icons": "^5.3.0"
  },
  "devDependencies": {
    "@sanity/pkg-utils": "^6.10.6",
    "@sanity/plugin-kit": "^4.0.17",
    "@types/react": "^18.3.3",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-sanity": "^7.1.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.34.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prettier": "^3.3.3",
    "prettier-plugin-packagejson": "^2.5.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sanity": "^3.51.0",
    "styled-components": "^6.1",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "react": "^18",
    "sanity": "^3"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Thread-Creative/thread-kit.git"
  },
  "bugs": {
    "url": "https://github.com/Thread-Creative/thread-kit/issues"
  },
  "homepage": "https://github.com/Thread-Creative/thread-kit#readme"
}
